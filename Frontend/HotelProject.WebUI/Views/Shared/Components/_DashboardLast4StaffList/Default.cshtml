@model List<ResultLast4StaffDto>
<div class="row">
    @foreach (var x in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@x.SocialMedia1" class="rounded-circle" width="50" height="50" alt="">
                        <h5 class="mt-3 mb-1">@x.Name</h5>
                        <p class="m-0">@x.Title</p>
                        <!-- <a href="javascript:void()" class="btn btn-sm btn-warning">Send Message</a> -->
                    </div>
                </div>
            </div>
        </div>
    }


</div>