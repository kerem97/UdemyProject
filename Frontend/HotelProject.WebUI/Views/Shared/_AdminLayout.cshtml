@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/AdminLayout/_HeadPartial.cshtml")
<body>
    @await Html.PartialAsync("/Views/AdminLayout/_PreloaderPartial.cshtml")
    <div id="main-wrapper">
        @await Html.PartialAsync("/Views/AdminLayout/_NavHeaderPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/_HeaderPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/_SidebarPartial.cshtml")
        <div class="content-body">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        @await Html.PartialAsync("/Views/AdminLayout/_FooterPartial.cshtml")
    </div>
    @await Html.PartialAsync("/Views/AdminLayout/_ScriptPartial.cshtml")
</body>
</html>
